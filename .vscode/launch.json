    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test hdhomerun",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${fileDirname}",
            "args": [
                "-test.v"
            ]
        },
        {
            "name": "Find",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "--host=192.168.1.1",
                "--debug=true",
                "find"
            ]
        },
        {
            "name": "Tune 593000",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}",
            "args": [
                "--host=192.168.1.1",
                "--debug=true",
                "tune",
                "--frequency=593000"
            ]
        },
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {},
            "args": []
        }
    ]
}